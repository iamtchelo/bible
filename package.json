{  "name": "biblejs",  "version": "1.0.0",  "description": "",  "main": "index.js",  "dependencies": {    "react": "^0.14.0",    "react-dom": "^0.14.0"  },  "devDependencies": {    "babel": "^5.8.23",    "babelify": "^6.3.0",    "browserify": "^11.0.1",    "envify": "^3.4.0",    "watchify": "^3.3.1"  },  "scripts": {    "start": "watchify -o public/assets/js/bundle.js -v -d app/js/app.js",    "build": "browserify app/js/app.js -t [envify --NODE_ENV production] | uglifyjs -cm | babel --optional optimisation.react.inlineElements --optional optimisation.react.constantElements > public/assets/js/bundle.min.js",    "test": "echo \"Error: no test specified\" && exit 1"  },  "browserify": {    "transform": [      [        "babelify",        {          "blacklist": [            "regenerator"          ]        }      ],      "envify"    ]  },  "repository": {    "type": "git",    "url": "git+https://github.com/marcelodeveloper/bible.git"  },  "keywords": [    "bible",    "js",    "god",    "jesus"  ],  "author": "Marcelo Silva <marcelosilva.developer@gmail.com>",  "license": "MIT",  "bugs": {    "url": "https://github.com/marcelodeveloper/bible/issues"  },  "homepage": "https://github.com/marcelodeveloper/bible#readme"}